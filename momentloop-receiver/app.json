{
  "expo": {
    "name": "MomentLoop Receiver",
    "slug": "momentloop-receiver",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8c79e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.momentloop.receiver"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e8c79e"
      },
      "package": "com.momentloop.receiver",
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#e8c79e"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      "react-native-cloud-storage"
    ],
    "extra": {
      "eas": {
        "projectId": "271b36e1-54be-45b1-85ba-34df184b7621"
      }
    },
    "owner": "pierrickm",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/development"
    }
  }
}
